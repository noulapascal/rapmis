{% extends 'base.html.twig' %}

{% block body %}
<div class="page-content-wrapper">
    <div class="page-content">
        <div class="page-bar">
            <div class="page-title-breadcrumb">
                <div class=" pull-left">
                    <div class="page-title">{% trans %} Dashboard {% endtrans %}</div>
                </div>
                <ol class="breadcrumb page-breadcrumb pull-right">
                    <li><i class="fa fa-home"></i>&nbsp;<a class="parent-item" href="{{ path('homepage') }}">{% trans %} Home {% endtrans %}</a>&nbsp;<i class="fa fa-angle-right"></i>
                    </li>
                    <li class="active">{% trans %} Dashboard {% endtrans %}</li>
                </ol>
            </div>
        </div>
    <h1>{% trans %}Type_etablissements list{% endtrans %}</h1>
        {% if is_granted('ROLE_SUPER_ADMIN') %}
    <table>
        <thead>
            <tr>
                <th>{% trans %} Id{% endtrans %}</th>
                <th>{% trans %} Name {% endtrans %}</th>
                <th>{% trans %}Description{% endtrans %}</th>
                <th>{% trans %}Actions{% endtrans %}</th>
            </tr>
        </thead>
        <tbody>
        {% for type_etablissement in type_etablissements %}
            <tr>
                <td><a href="{{ path('type_etablissements_show', { 'id': type_etablissement.id }) }}">{{ type_etablissement.id }}</a></td>
                <td>{{ type_etablissement.name }}</td>
                <td>{{ type_etablissement.description }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('type_etablissements_show', { 'id': type_etablissement.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('type_etablissements_edit', { 'id': type_etablissement.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
        {% endif %}
    <ul>
        <li>
            <a href="{{ path('type_etablissements_new') }}">Create a new type_etablissement</a>
        </li>
    </ul>
    </div>
</div>
{% endblock %}
